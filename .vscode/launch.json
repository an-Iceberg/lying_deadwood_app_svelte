{
  "configurations": [
    {
      "name": "Run tauri app",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo tauri dev",
      "showTerminal": true,
      "terminalName": "Tauri"
    },
    {
      "name": "Build tauri app",
      "type": "f5anything",
      "request": "launch",
      "command": "NO_STRIP=true cargo tauri build",
      "showTerminal": true,
      "terminalName": "Tauri"
    },
    {
      "name": "Build android debug APK",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo tauri android build --debug --apk --split-per-abi",
      "showTerminal": true,
      "terminalName": "Tauri"
    },
  ]
}
