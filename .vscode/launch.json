{
  "configurations": [
    // Todo: https://stackoverflow.com/questions/71272054/how-to-run-multiple-commands-at-once-asynchronously-in-fish-shell
    {
      "name": "Run tauri app",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo tauri dev",
      "showTerminal": true,
      "terminalName": "Tauri"
    },
    {
      "name": "Build tauri app",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo tauri build --no-bundle",
      "showTerminal": true,
      "terminalName": "Tauri"
    },
    {
      "name": "Bundle tauri app",
      "type": "f5anything",
      "request": "launch",
      "command": "NO_STRIP=true cargo tauri build",
      "showTerminal": true,
      "terminalName": "Tauri"
    },
    {
      "name": "Build android debug APK",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo tauri android build --debug --apk --split-per-abi",
      "showTerminal": true,
      "terminalName": "Tauri"
    },
  ]
}
